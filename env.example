# ImageGenAI Environment Configuration
# Copy this file to env.dev and fill in your actual values

# Application Configuration
NODE_ENV=development
APP_NAME=ImageGenAI

# Backend Configuration
HOST=0.0.0.0
PORT=8000
DEBUG=true
GOOGLE_AI_API_KEY=your_google_ai_api_key_here
FRONTEND_URL=http://localhost:5001
ALLOWED_ORIGINS=["http://localhost:5001","http://localhost:6001","http://127.0.0.1:5001","http://127.0.0.1:6001"]
MAX_FILE_SIZE=10485760
ALLOWED_IMAGE_TYPES=["image/jpeg","image/jpg","image/png","image/webp"]

# Frontend Configuration
NEXT_PUBLIC_API_URL=http://localhost:6001/api
FRONTEND_PORT=5001
BACKEND_PORT=6001
